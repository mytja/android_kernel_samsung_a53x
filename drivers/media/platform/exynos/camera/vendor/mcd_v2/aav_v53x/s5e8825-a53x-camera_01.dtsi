		/* 20210405_S21FE_UW_12M_Hi1336_OTP_Cal map V008.001_QC_LSI_Map */
		/* 0 header */
		header_crc_check_list = <0x0 0x4B 0x4C>;
		/* 0 module */
		crc_check_list = <0x50 0x4CB 0x4CC>;
		/* Master */
		rom_header_version_start_addr = <0x0A>;
		rom_header_cal_data_start_addr = <0x50>;
		rom_header_cal_map_ver_start_addr = <0x16>;
		//rom_header_isp_setfile_ver_start_addr = <0x54>;
		rom_header_project_name_start_addr = <0x1E>;
		rom_header_module_id_addr = <0x2E>; /* 0x28 + reserved bytes */
		rom_header_sensor_id_addr = <0x38>;
		rom_awb_master_addr = <0x50>;

		/* standard cal */
		use_standard_cal;
		rom_standard_cal_start_addr = <0x50>;
		rom_standard_cal_end_addr = <0x04CF>;
		rom_standard_cal_sec2lsi_end_addr = <0x1ADF>;
		rom_awb_start_addr = <0x50>;
		rom_awb_end_addr = <0x57>;
		rom_shading_start_addr = <0x5B>;
		rom_shading_end_addr = <0x460>;
		rom_factory_start_addr = <0x04D0>;
		rom_factory_end_addr = <0x04EF>;
		rom_awb_sec2lsi_start_addr = <0x50>;
		rom_awb_sec2lsi_end_addr = <0x57>;
		rom_awb_sec2lsi_checksum_addr = <0x5C>;
		rom_awb_sec2lsi_checksum_len = <0xC>;
		rom_shading_sec2lsi_start_addr = <0x60>;
		rom_shading_sec2lsi_end_addr = <0x1A47>;
		rom_shading_sec2lsi_checksum_addr = <0x1A4C>;
		rom_shading_sec2lsi_checksum_len = <0x19EC>;
		rom_factory_sec2lsi_start_addr = <0x1AC0>;
	};	

	rear2_otprom@6E { /* Bokeh */
		compatible = "samsung,rear2-otprom-i2c";
		reg = <0x01>;

		rom_power_position = <2>;
		rom_size = <0x1AE0>;

		cal_map_es_version = <1>;
		camera_module_es_version = "A";
		/* skip_cal_loading;*/

		/* 0 header */
		header_crc_check_list = <0x0 0x6B 0x6C>;
		/* Master */
		rom_header_version_start_addr = <0x00>;
		rom_header_cal_map_ver_start_addr = <0x1C>;
		rom_header_project_name_start_addr = <0x24>;
		rom_header_module_id_addr = <0x46>; /* 0x28 + reserved bytes */
		rom_header_sensor_id_addr = <0x50>;
	};

	rear4_otprom@7E { /* Macro otprom */
		compatible = "samsung,rear4-otprom-i2c";
		reg = <0x02>;

		rom_power_position = <6>;
		rom_size = <0x1A50>;
		cal_map_es_version = <1>;
 		camera_module_es_version = "A";
		/* skip_cal_loading;*/

		/* 0 header */
		header_crc_check_list = <0x0 0x4B 0x4C>;
		/* 0 module */
		crc_check_list = <0x50 0xEF 0xF0>;
		/* Master */
		rom_header_version_start_addr = <0x0A>;
		rom_header_cal_data_start_addr = <0x50>;
		rom_header_cal_map_ver_start_addr = <0x16>;
		rom_header_project_name_start_addr = <0x1E>;
		rom_header_module_id_addr = <0x2E>; /* 0x28 + reserved bytes */
		rom_header_sensor_id_addr = <0x38>;
		rom_awb_master_addr = <0x50>;

		use_standard_cal;
		rom_standard_cal_start_addr = <0x50>;
		rom_standard_cal_end_addr = <0xF3>;
		rom_standard_cal_sec2lsi_end_addr = <0xF3>;
		rom_awb_start_addr = <0x50>;
		rom_awb_end_addr = <0x57>;
		rom_shading_start_addr = <0x60>;
		rom_shading_end_addr = <0xE8>;
		rom_awb_sec2lsi_start_addr = <0x50>;
		rom_awb_sec2lsi_end_addr = <0x57>;
		rom_awb_sec2lsi_checksum_addr = <0x5C>;
