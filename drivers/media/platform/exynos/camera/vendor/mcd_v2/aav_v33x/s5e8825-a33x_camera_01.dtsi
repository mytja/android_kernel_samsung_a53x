#include <dt-bindings/camera/exynos_is_dt.h>

#include "module_imx582.dtsi"
#include "module_4ha.dtsi"
#include "module_3l6.dtsi"
#include "module_imx355.dtsi"
#include "module_gc02m1.dtsi"
#include "module_imx258.dtsi"
#include "module_gc5035.dtsi"

/ {
	fragment@camera {
		target-path = "/";
		__overlay__ {
			#address-cells = <2>;
			#size-cells = <1>;
			interrupt-parent = <&gic>;

			is_sensor_imx582: is_sensor_imx582@34 {
				power_seq {
					scenario_normal_on {
						40 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
					};

					scenario_normal_off {
						80 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN1 0>; };
					};

					scenario_ois_factory_on {
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
					};

					scenario_ois_factory_off {
						80 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN1 0>; };
					};

					scenario_read_rom_on {
						10 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 7000>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
					};

					scenario_read_rom_off {
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 20000>; share = <SRT_RELEASE SHARED_PIN1 0>; };
					};
				};
			};

			is_sensor_imx355: is_sensor_imx355@20 {
				power_seq {
					scenario_normal_on {
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 2000>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						80 { pname = "MCLK"; pin = <PIN_MCLK 1 1000>; gpio = <&gpg0 4 0x1>; share = <SRT_ACQUIRE SHARED_PIN2 1>; };
					};

					scenario_normal_off {
						50 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN1 0>; };
						60 { pname = "MCLK"; pin = <PIN_MCLK 0 0>; gpio = <&gpg0 4 0x1>; share = <SRT_RELEASE SHARED_PIN2 0>;};
					};

					scenario_read_rom_on {
						10 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 7000>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
					};

					scenario_read_rom_off {
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 20000>; share = <SRT_RELEASE SHARED_PIN1 0>; };
					};
				};
			};

			is_sensor_4ha: is_sensor_4ha@5A {
				power_seq {
					scenario_normal_on {
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 2000>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						80 { pname = "MCLK"; pin = <PIN_MCLK 1 1000>; gpio = <&gpg0 4 0x1>; share = <SRT_ACQUIRE SHARED_PIN2 1>; };
					};

					scenario_normal_off {
						50 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN1 0>; };
						60 { pname = "MCLK"; pin = <PIN_MCLK 0 0>; gpio = <&gpg0 4 0x1>; share = <SRT_RELEASE SHARED_PIN2 0>;};
					};

					scenario_read_rom_on {
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 2000>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						80 { pname = "MCLK"; pin = <PIN_MCLK 1 1000>; gpio = <&gpg0 4 0x1>; share = <SRT_ACQUIRE SHARED_PIN2 1>; };
					};

					scenario_read_rom_off {
						50 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN1 0>; };
						60 { pname = "MCLK"; pin = <PIN_MCLK 0 0>; gpio = <&gpg0 4 0x1>; share = <SRT_RELEASE SHARED_PIN2 0>;};
					};

				};
			};

			is_sensor_gc02m1: is_sensor_gc02m1@6E {
				power_seq {
					scenario_normal_on {
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						40 { pname = "rcam2_rcam4_ldo_2p8_en"; pin = <PIN_OUTPUT 1 300>; gpio = <&gpm6 0 0x1>; share = <SRT_ACQUIRE SHARED_PIN4 1>;};
						80 { pname = "MCLK"; pin = <PIN_MCLK 1 0>; gpio = <&gpg0 4 0x1>; share = <SRT_ACQUIRE SHARED_PIN2 1>; };
					};

					scenario_normal_off {
						40 { pname = "rcam2_rcam4_ldo_2p8_en"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpm6 0 0x1>; share = <SRT_RELEASE SHARED_PIN4 0>; };
						50 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN1 0>; };
						60 { pname = "MCLK"; pin = <PIN_MCLK 0 200>; gpio = <&gpg0 4 0x1>; share = <SRT_RELEASE SHARED_PIN2 0>;};
					};

					scenario_read_rom_on {
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						40 { pname = "rcam2_rcam4_ldo_2p8_en"; pin = <PIN_OUTPUT 1 1000>; gpio = <&gpm6 0 0x1>; share = <SRT_ACQUIRE SHARED_PIN4 1>;};
						80 { pname = "MCLK"; pin = <PIN_MCLK 1 500>; gpio = <&gpg0 4 0x1>; share = <SRT_ACQUIRE SHARED_PIN2 1>; };
					};

					scenario_read_rom_off {
						40 { pname = "rcam2_rcam4_ldo_2p8_en"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpm6 0 0x1>; share = <SRT_RELEASE SHARED_PIN4 0>; };
						50 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN1 0>; };
						60 { pname = "MCLK"; pin = <PIN_MCLK 0 200>; gpio = <&gpg0 4 0x1>; share = <SRT_RELEASE SHARED_PIN2 0>;};
					};

				};
			};

			is_sensor_imx258: is_sensor_imx258@34 {
				power_seq {
					scenario_normal_on {
						30 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						40 { pname = "VDDD_1.2V_SUB"; pin = <PIN_REGULATOR 1 500 1200000>; };
						70 { pname = "MCLK"; pin = <PIN_MCLK 1 500>; gpio = <&gpg0 3 0x1>; share = <SRT_ACQUIRE SHARED_PIN3 1>;};
					};

					scenario_normal_off {
						40 { pname = "MCLK"; pin = <PIN_MCLK 0 0>; gpio = <&gpg0 3 0x1>; share = <SRT_RELEASE SHARED_PIN3 0>; };
						60 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN1 0>; };
					};
					scenario_read_rom_on {
						10 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 7000>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
					};
					scenario_read_rom_off {
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 20000>; share = <SRT_RELEASE SHARED_PIN1 0>; };
					};
				};
			};

			is_sensor_3l6: is_sensor_3l6@5A {
				power_seq {
					scenario_normal_on {
						30 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						40 { pname = "VDDD_1.2V_SUB"; pin = <PIN_REGULATOR 1 1000 1050000>; };
						70 { pname = "MCLK"; pin = <PIN_MCLK 1 500>; gpio = <&gpg0 3 0x1>; share = <SRT_ACQUIRE SHARED_PIN3 1>;};
					};

					scenario_normal_off {
						40 { pname = "MCLK"; pin = <PIN_MCLK 0 0>; gpio = <&gpg0 3 0x1>; share = <SRT_RELEASE SHARED_PIN3 0>; };
						90 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN1 0>; };
					};
					scenario_read_rom_on {
						10 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 7000>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
					};
					scenario_read_rom_off {
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 20000>; share = <SRT_RELEASE SHARED_PIN1 0>; };
					};
				};
			};

			is_sensor_gc5035_macro: is_sensor_gc5035_macro@7E {
				power_seq {
					scenario_normal_on {
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						40 { pname = "rcam2_rcam4_ldo_2p8_en"; pin = <PIN_OUTPUT 1 300>; gpio = <&gpm6 0 0x1>; share = <SRT_ACQUIRE SHARED_PIN4 1>; };
						80 { pname = "MCLK"; pin = <PIN_MCLK 1 1500>; gpio = <&gpg0 3 0x1>; share = <SRT_ACQUIRE SHARED_PIN3 1>;};
					};

					scenario_normal_off {
						30 { pname = "rcam2_rcam4_ldo_2p8_en"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpm6 0 0x1>; share = <SRT_RELEASE SHARED_PIN4 0>; };
						50 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN1 0>; };
						60 { pname = "MCLK"; pin = <PIN_MCLK 0 0>; gpio = <&gpg0 3 0x1>; share = <SRT_RELEASE SHARED_PIN3 0>; };
					};

					scenario_read_rom_on {
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						40 { pname = "rcam2_rcam4_ldo_2p8_en"; pin = <PIN_OUTPUT 1 300>; gpio = <&gpm6 0 0x1>; share = <SRT_ACQUIRE SHARED_PIN4 1>; };
						80 { pname = "MCLK"; pin = <PIN_MCLK 1 1500>; gpio = <&gpg0 3 0x1>; share = <SRT_ACQUIRE SHARED_PIN3 1>;};
					};

					scenario_read_rom_off {
						30 { pname = "rcam2_rcam4_ldo_2p8_en"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpm6 0 0x1>; share = <SRT_RELEASE SHARED_PIN4 0>; };
						50 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN1 0>; };
						60 { pname = "MCLK"; pin = <PIN_MCLK 0 0>; gpio = <&gpg0 3 0x1>; share = <SRT_RELEASE SHARED_PIN3 0>; };
					};

				};
			};

			is_flash_s2mu106: is-flash-s2mu106@0 {
				compatible = "samsung,sensor-flash-s2mu106";
				sensor-module = <&is_sensor_imx582>,<&is_sensor_gc5035_macro>;
				id = <0 3>;
				led_ch = <1 2>;
				status = "okay";
			};

			d_phy_setfile_w_uw: d_phy_setfile_w_uw@00 {
				comm {
					/* bias */
					0  = <0x1000 0 32 0x00000010 IDX_BIA_VAL 0>; /* M_BIAS_CON0 */
					1  = <0x1004 0 32 0x00000110 IDX_BIA_VAL 0>; /* M_BIAS_CON1 */
					2  = <0x1008 0 32 0x00003223 IDX_BIA_VAL 0>; /* M_BIAS_CON2 */
					3  = <0x1010 0 32 0x00000200 IDX_BIA_VAL 0>; /* M_BIAS_CON4 */

					/* clock */
					4 = <0x0004 0 32 0x00001450 IDX_FIX_VAL 0>; /* SC_GNR_CON1 */
					5 = <0x0008 0 32 0x0000000C IDX_FIX_VAL 0>; /* SC_ANA_CON0 */
					6 = <0x000C 0 32 0x0000F343 IDX_FIX_VAL 0>; /* SC_ANA_CON1 */
					7 = <0x0010 0 32 0x00000002 IDX_FIX_VAL 0>; /* SC_ANA_CON2 */
					8 = <0x0014 0 32 0x00008600 IDX_FIX_VAL 0>; /* SC_ANA_CON3 */
					9 = <0x0018 0 32 0x00004000 IDX_FIX_VAL 0>; /* SC_ANA_CON4 */
					10 = <0x0030 0 32 0x00000301 IDX_FIX_VAL 0>; /* SC_TIME_CON0 */
					/* Enable should be set at last. */
					11 = <0x0000 0 32 0x00000001 IDX_FIX_VAL 0>; /* SC_GNR_CON0, Phy clock enable */
				};

				lane {
					0  = <0x0004 0 32 0x00001450 IDX_FIX_VAL 4>; /* SD_GNR_CON1 */
					1  = <0x0008 0 32 0x0000000C IDX_FIX_VAL 4>; /* SD_ANA_CON0 */
					2  = <0x000C 0 32 0x0000F343 IDX_FIX_VAL 4>; /* SD_ANA_CON1 */
					3  = <0x0010 0 32 0x00000002 IDX_FIX_VAL 4>; /* SD_ANA_CON2 */
					4  = <0x0010 8  2 0x00000000 IDX_SKW_DLY 4>; /* SD_ANA_CON2 */
					5  = <0x0014 0 32 0x00008600 IDX_FIX_VAL 4>; /* SD_ANA_CON3 */
					6  = <0x0018 0 32 0x00004000 IDX_FIX_VAL 4>; /* SD_ANA_CON4 */
					7  = <0x0024 0 32 0x00000040 IDX_FIX_VAL 4>; /* SD_ANA_CON7 */
					8  = <0x0030 0  8 0x00000000 IDX_STL_VAL 4>; /* SD_TIME_CON0 */
					9  = <0x0030 8  1 0x00000000 IDX_STL_CLK 4>; /* SD_TIME_CON0 */
					10 = <0x0034 0 32 0x00000003 IDX_FIX_VAL 4>; /* SD_TIME_CON1 */
					11 = <0x0040 0  1 0x00000001 IDX_SKW_CAL 4>; /* SD_DESKEW_CON0 */
					12 = <0x0050 0 32 0x0000081A IDX_FIX_VAL 4>; /* SD_DESKEW_CON4 */
					/* Enable should be set at last. */
					13 = <0x0000 0 32 0x00000001 IDX_FIX_VAL 4>; /* SD_GNR_CON0 , Phy data enable */
				};
			};

			/* REAR CAMERA */
			is_sensor0: is_sensor0@15030000 {
				#address-cells = <1>;
				#size-cells = <1>;
				scenario = <SENSOR_SCENARIO_NORMAL>;	/* Normal, Vision, OIS etc */
				id = <0>;
				csi_ch = <0>;
				status = "okay";
				use_cphy = <0>;

				interrupts = <0 INTREQ__CSIS0 IRQ_TYPE_LEVEL_HIGH>; /*MIPI-CSI0 */
				interrupt-names = "csi";
				phys = <&mipi_phy_csis0_m0s4s4s4s4s2s1 0>;
				phy_setfile = <&d_phy_setfile_w_uw>;
			};

			/* Bokeh CAMERA */
			is_sensor1: is_sensor1@15040000 {
				#address-cells = <1>;
				#size-cells = <1>;
				reg = <0x0 0x15060000 0x1000>, /* MIPI-CSI3 */
					<0x0 0x150F0000 0x10000>; /* PHY: M0S4S4S4S4S2S1 */
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <2>;
				csi_ch = <3>;
				status = "okay";
				use_cphy = <0>;

				interrupts = <0 INTREQ__CSIS3 IRQ_TYPE_LEVEL_HIGH>; /* MIPI-CSI3 */
				interrupt-names = "csi";
				phys = <&mipi_phy_csis3_m0s4s4s4s4s2s1 0>;
				phy_setfile = <&d_phy_setfile>;
			};

			/* UW CAMERA */
			is_sensor2: is_sensor2@15050000 {
				#address-cells = <1>;
				#size-cells = <1>;
				reg = <0x0 0x15040000 0x1000>, /* MIPI-CSI1 */
					<0x0 0x150F0000 0x10000>; /* PHY: M0S4S4S4S4S2S1 */
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <4>;
				csi_ch = <1>;
				status = "okay";

				interrupts = <0 INTREQ__CSIS1 IRQ_TYPE_LEVEL_HIGH>; /* MIPI-CSI1 */
				interrupt-names = "csi";
				phys = <&mipi_phy_csis1_m0s4s4s4s4s2s1 0>;
				phy_setfile = <&d_phy_setfile_w_uw>;
			};

			/* Front CAMERA */
			is_sensor4: is_sensor4@15070000 {
				#address-cells = <1>;
				#size-cells = <1>;
				reg = <0x0 0x15050000 0x1000>, /* MIPI-CSI2 */
					<0x0 0x150F0000 0x10000>; /* PHY: M0S4S4S4S4S2S1 */
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <1>;
				csi_ch = <2>;
				status = "okay";

				interrupts = <0 INTREQ__CSIS2 IRQ_TYPE_LEVEL_HIGH>; /* MIPI-CSI2 */
				phys = <&mipi_phy_csis2_m0s4s4s4s4s2s1 0>;
				phy_setfile = <&d_phy_setfile>;
			};

			/* Macro CAMERA */
			is_sensor3: is_sensor3@15060000 {
				#address-cells = <1>;
				#size-cells = <1>;
				reg = <0x0 0x15070000 0x1000>, /* MIPI-CSI4 */
					<0x0 0x150F0000 0x10000>; /* PHY: M0S4S4S4S4S2S1 */
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <3>;
				csi_ch = <4>;
				status = "okay";

				interrupts = <0 INTREQ__CSIS4 IRQ_TYPE_LEVEL_HIGH>; /* MIPI-CSI2 */
				phys = <&mipi_phy_csis4_m0s4s4s4s4s2s1 0>;
				phy_setfile = <&phy_setfile_e>;
			};
		}; /* end of __overlay__ */
	}; /* end of fragment */
}; /* end of root */

/***** reference symbol node *****/
/* GPIO_PERI */
&pinctrl_1 {
	/* FRONT I2C */
	hsi2c29_bus_in: hsi2c29-bus-in {
		samsung,pins = "gpm17-0", "gpm18-0";
		samsung,pin-function = <0>; /* 0: Input, 1: Output */
		samsung,pin-pud = <0>;      /* 0: NP, 1: PD, 2: Reserved, 3: PU */
		samsung,pin-drv = <0>;      /* 0: x1, 1: x2, 2: x3, 3: x4 */
	};

	/* REAR I2C */
	hsi2c21_bus_in: hsi2c21-bus-in {
		samsung,pins = "gpm0-0", "gpm1-0";
		samsung,pin-function = <0>; /* 0: Input, 1: Output */
		samsung,pin-pud = <0>;      /* 0: NP, 1: PD, 2: Reserved, 3: PU */
		samsung,pin-drv = <0>;      /* 0: x1, 1: x2, 2: x3, 3: x4 */
	};

	/* BOKEH_UW_MACRO I2C */
	hsi2c22_bus_in: hsi2c22-bus-in {
		samsung,pins = "gpm2-0", "gpm3-0";
		samsung,pin-function = <0>; /* 0: Input, 1: Output */
		samsung,pin-pud = <0>;      /* 0: NP, 1: PD, 2: Reserved, 3: PU */
		samsung,pin-drv = <0>;      /* 0: x1, 1: x2, 2: x3, 3: x4 */
	};

	/* AF_EEP I2C */
	hsi2c23_bus_in: hsi2c23-bus-in {
		samsung,pins = "gpm4-0", "gpm5-0";
		samsung,pin-function = <0>; /* 0: Input, 1: Output */
		samsung,pin-pud = <0>;      /* 0: NP, 1: PD, 2: Reserved, 3: PU */
		samsung,pin-drv = <0>;      /* 0: x1, 1: x2, 2: x3, 3: x4 */
	};

	/* MCU I2C */
	hsi2c25_bus: hsi2c25-bus {
		samsung,pins = "gpm8-0", "gpm9-0";
		samsung,pin-function = <2>;
		samsung,pin-pud = <3>;
		samsung,pin-drv = <0>;
	};

	ldo_gpio_en: ldo-gpio-en {
		samsung,pins = "gpm6-0", "gpm11-0", "gpm19-0";
		samsung,pin-function = <0>; /* 0: Input, 1: Output */
		samsung,pin-pud = <1>;      /* 0: NP, 1: PD, 2: Reserved, 3: PU */
		samsung,pin-con-pdn = <2>;  /* 0: Output0, 1: Output1, 2: Input, 3:Prev */
		samsung,pin-pud-pdn = <1>;  /* 0: NP, 1: PD, 2: Reserved, 3: PU */
	};

	fcam_rcam4_rst_n: fcam-rcam4-rst-n {
		samsung,pins = "gpm13-0", "gpm20-0";
		samsung,pin-function = <1>; /* 0: Input, 1: Output */
		samsung,pin-pud = <1>;      /* 0: NP, 1: PD, 2: Reserved, 3: PU */
		samsung,pin-con-pdn = <0>;  /* 0: Output0, 1: Output1, 2: Input, 3:Prev */
		samsung,pin-pud-pdn = <1>;  /* 0: NP, 1: PD, 2: Reserved, 3: PU */
	};
};

&hsi2c_29 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpm17 0 0 &gpm18 0 0>;
	status = "okay";
	clock-frequency = <1000000>;
	samsung,reset-before-trans;
	samsung,fast-plus-mode;

	pinctrl-names = "default","on_i2c","off_i2c";
	pinctrl-0 = <&hsi2c29_bus_in>;
	pinctrl-1 = <&hsi2c29_bus>;
	pinctrl-2 = <&hsi2c29_bus_in>;

	is-imx258@1A {
		compatible = "samsung,exynos-is-cis-imx258";
		sensor-module = <&is_sensor_imx258>;
		reg = <0x1A>; /* 1 bit right shift */
		id = <1>; /* matching is_sensor id */
		sensor_f_number = <220>;	/* f number 2.2 */
		setfile = "setB";
		use_initial_ae; /* for initial AE of face unlock */
		status = "okay";
	};

	is-3l6@5A{
		compatible = "samsung,exynos-is-cis-3l6";
		sensor-module = <&is_sensor_3l6>;
		reg = <0x2D>; /* 1 bit right shift */
		id = <1>; /* matching is_sensor id */
		sensor_f_number = <220>;	/* f number 2.2 */
		setfile = "setB";
		use_initial_ae; /* for initial AE of face unlock */
		status = "okay";
	};
};

