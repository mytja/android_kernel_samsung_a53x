						vc2 = </* in */ 1 HW_FORMAT_RAW10_POTF 3968 558	/* out */ HW_FORMAT_RAW10_POTF VC_TAILPDAF 3968 558>;
						vc3 = </* in */ 3 HW_FORMAT_UNKNOWN 0 0	/* out */ HW_FORMAT_UNKNOWN VC_NOTHING 0 0>;
