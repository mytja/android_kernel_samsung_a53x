#include <dt-bindings/camera/exynos_is_dt.h>

#include "module_gn3.dtsi"
#include "module_2la.dtsi"
#include "module_3k1.dtsi"
#include "module_imx374.dtsi"

/ {
	fragment@camera {
		target-path = "/";
		__overlay__ {
			#address-cells = <2>;
			#size-cells = <1>;
			interrupt-parent = <&gic>;

			is_flash_gpio: is-flash-gpio@0 {
				compatible = "samsung,sensor-flash-s2mpb02";
				sensor-module = <&is_sensor_gn3>, <&is_sensor_3k1>;
				id = <0 2>;
				status = "okay";

				torch-gpio = <&gpc4 0 0x1>;
			};

			/* OIS_MCU */
			ois_mcu: ois_mcu@173F0000 {
				pinctrl-names = "default";
				pinctrl-0 = <&ois_spi0 &ois_spi1>;
				compatible = "samsung,sensor-ois-mcu";
				reg = <0x0 0x173F0000 0x10000>, /* OIS MCU (SRAM 56KB) */
					<0x0 0x11DD0000 0x10000>, /* I2C: USI05_USI */
					<0x0 0x11DE0000 0x10000>,	/* SPI: USI06_USI */
					<0x0 0x11C30000 0x1000>;	/* PERI setting */
				reg-names = "core", "i2c", "spi", "peri";
				interrupts = <0 INTREQ__OIS_MCU_CSIS IRQ_TYPE_LEVEL_HIGH>;
				id = <0 2>;
				status = "okay";
				mcu_ctrl_actuator = <0 1>;
				mcu_support_photo_fastae;

				power-domains = <&pd_allcsis>;
				clocks = <&clock GATE_OIS_MCU_TOP_QCH>, <&clock DOUT_DIV_CLK_PERIC2_USI06>;
				clock-names = "user_mux", "ipclk_spi";
			};

			/* REAR CAMERA */
			is_sensor0: is_sensor0@17030000 {
				#address-cells = <1>;
				#size-cells = <1>;
				reg = <0x0 0x17050000 0x1000>, /* MIPI-CSI2 */
					<0x0 0x170F0000 0x10000>, /* PHY: M0S4S4S4S4S4S2 */
					<0x0 0x17150050 0x10>; /* CSIS_FRO */
				scenario = <SENSOR_SCENARIO_NORMAL>;	/* Normal, Vision, OIS etc */
				id = <0>;
				csi_ch = <2>;
				status = "okay";
				use_cphy = <1>;

				interrupts = <0 INTREQ__CSIS2 IRQ_TYPE_LEVEL_HIGH>;
				phys = <&mipi_phy_csis2_m0s4s4s4s4s4s4_s22 0>;
				phy_setfile = <&cphy_gn3_setfile>;
				phy_tune_default_mode = "4080 3060 60";
			};

			/* FRONT CAMERA */
			is_sensor1: is_sensor1@17040000 {
				#address-cells = <1>;
				#size-cells = <1>;
				reg = <0x0 0x17030000 0x1000>, /* MIPI-CSI0 */
					<0x0 0x170F0000 0x10000>, /* PHY: M0S4S4S4S4S4S2 */
					<0x0 0x17150050 0x10>; /* CSIS_FRO */
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <1>;
				csi_ch = <0>;
				status = "okay";
				use_cphy = <0>;

				interrupts = <0 INTREQ__CSIS0 IRQ_TYPE_LEVEL_HIGH>;
				phys = <&mipi_phy_csis0_m0s4s4s4s4s4s4_s22 0>;
			};

			/* Tele CAMERA */
			is_sensor2: is_sensor2@17050000 {
				#address-cells = <1>;
				#size-cells = <1>;
				reg = <0x0 0x17060000 0x1000>, /* MIPI-CSI3 */
					<0x0 0x170F0000 0x10000>, /* PHY: M0S4S4S4S4S4S2 */
					<0x0 0x17150050 0x10>; /* CSIS_FRO */
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <2>;
				csi_ch = <3>;
				status = "okay";
				use_cphy = <0>;

				interrupts = <0 INTREQ__CSIS3 IRQ_TYPE_LEVEL_HIGH>;
				phys = <&mipi_phy_csis3_m0s4s4s4s4s4s4_s22 0>;
			};

			is_sensor3: is_sensor3@17060000 {
				#address-cells = <1>;
				#size-cells = <1>;
				scenario = <SENSOR_SCENARIO_VISION>;    /* Normal, Vision, OIS etc */
				id = <3>;
				csi_ch = <4>;
				status = "okay";
			};

			/* ULTRA WIDE CAMERA */
			is_sensor4: is_sensor4@17070000 {
				#address-cells = <1>;
				#size-cells = <1>;
				reg = <0x0 0x17040000 0x1000>, /* MIPI-CSI1 */
					<0x0 0x170F0000 0x10000>, /* PHY: M0S4S4S4S4S4S2 */
					<0x0 0x17150050 0x10>; /* CSIS_FRO */
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <4>;
				csi_ch = <1>;
				status = "okay";
				use_cphy = <1>;

				interrupts = <0 INTREQ__CSIS1 IRQ_TYPE_LEVEL_HIGH>;
				phys = <&mipi_phy_csis1_m0s4s4s4s4s4s4_s22 0>;
				phy_setfile = <&cphy_2la_setfile>;
				phy_tune_default_mode = "2016 1134 120";
			};

			is_sensor5: is_sensor5@17080000 {
				#address-cells = <1>;
				#size-cells = <1>;
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <1>;
				csi_ch = <1>;
				status = "disabled";
			};
		}; /* end of __overlay__ */
	}; /* end of fragment */
}; /* end of root */

/***** reference symbol node *****/
&pinctrl_5 {
	wide_rst_sleep: wide-rst-sleep {
		samsung,pins = "gpc0-0";
		samsung,pin-con-pdn = <0>;
	};

	uwide_rst_sleep: uwide-rst-sleep {
		samsung,pins = "gpc0-1";
		samsung,pin-con-pdn = <0>;
	};
};

&pinctrl_6 {
	/* REAR WIDE */
	hsi2c16_bus_in: hsi2c16-bus-in {
		samsung,pins = "gpp8-0", "gpp8-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
		samsung,pin-pud-pdn = <0>;
	};

	/* REAR WIDE AF */
	hsi2c14_bus_in: hsi2c14-bus-in {
		samsung,pins = "gpp7-0", "gpp7-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
		samsung,pin-pud-pdn = <0>;
	};

	/* REAR TELE */
	hsi2c17_bus_in: hsi2c17-bus-in {
		samsung,pins = "gpp8-2", "gpp8-3";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
		samsung,pin-pud-pdn = <0>;
	};

