&hsi2c_30 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpm6 0 0 &gpm6 1 0>;
	status = "okay";
	clock-frequency = <1000000>;
	samsung,reset-before-trans;
	samsung,fast-plus-mode;

	pinctrl-names = "default","on_i2c","off_i2c";
	pinctrl-0 = <&hsi2c30_bus_in>;
	pinctrl-1 = <&hsi2c30_bus>;
	pinctrl-2 = <&hsi2c30_bus_in>;

	is-2la@10 { /* ultrawide */
		compatible = "samsung,exynos-is-cis-2la";
		sensor-module = <&is_sensor_2la>;
		reg = <0x10>; /* 1 bit right shift */
		id = <4>; /* matching fimc_is_sensor id */
		setfile = "setA";
		status = "okay";
		rev_reg = <0x0002 2 0xA202>;
		vendor_use_adaptive_mipi;
	};

	rear2_eeprom@51 { /* Ultra wide eeprom */
		compatible = "samsung,rear2-eeprom-i2c";
		reg = <0x51>;

		rom_power_position = <4>;
		rom_size = <0x4000>;
		cal_map_es_version = <23>;
		camera_module_es_version = "A";
		/* skip_cal_loading; */

		/* Rainbow_project_R0G0_Rear_Cal_Map_V023_20210520_for_all */
		/* 0 header */
		header_crc_check_list = <0x0000 0x00FB 0x00FC>;
		/* 0 master LSC */
		/* 1 master all */
		crc_check_list = <0x0100 0x14FB 0x14FC
			0x2C60 0x2C9B 0x2C9C
			0x2CA0 0x2D0B 0x2D0C
			0x1C00 0x2DDF 0x2DFC>;
		/* Master */
		rom_header_version_start_addr = <0x40>;
		rom_header_cal_data_start_addr = <0x100>;
		rom_header_cal_map_ver_start_addr = <0x50>;
		rom_header_isp_setfile_ver_start_addr = <0x54>;
		rom_header_project_name_start_addr = <0x5E>;
		rom_header_module_id_addr = <0xAE>; /* 0xA8 + reserved bytes */
		rom_header_sensor_id_addr = <0xB8>;
		rom_header_mtf_data_addr = <0x66>;
		rom_awb_master_addr = <0x2C60>;
		rom_awb_module_addr = <0x2C84>;
	};
};

