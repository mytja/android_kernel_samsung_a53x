	pinctrl-2 = <&hsi2c14_bus_in>;

	is-actuator@0C { /* wide AF */
		compatible = "samsung,exynos-is-actuator-ak737x";
		sensor-module = <&is_sensor_gn3>;
		reg = <0x0C>;
		id = <0>; /* matching is_sensor id */
		/* AK737x: addr 0xF3, data 0x0E */
		vendor_product_id = <0xF3 0x0E 0xF3 0x13 0xF3 0x00>;
		vendor_first_pos = <2000>;  /* 12bits */
		vendor_first_delay = <2000>;
		vendor_soft_landing_seqid = <2>;
		vendor_soft_landing_list = <640 30 64 30>; /* 1st_pos delay 2nd_pos delay */  /* 12bits */
		vendor_use_sleep_mode;
		vendor_use_standby_mode; /* AK7377 with vendor_use_sleep_mode */
	};

	rear_eeprom@50 {
		compatible = "samsung,rear-eeprom-i2c";
		reg = <0x50>;
		rom_power_position = <0>;
		rom_size = <0x8000>;
		cal_map_es_version = <23>;
		camera_module_es_version = "A";
		/* skip_cal_loading; */

		/* Rainbow_project_R0G0_Rear_Cal_Map_V023_20210520_for_all */
		/* 0 header */
		header_crc_check_list = <0x0000 0x01FB 0x01FC>;
		/* 0 LSI Wide LSC data */
		/* 1 Wide camera */
		/* 2 ATC data */
		/* 3 Tele camera */
		/* 4 Sensor */
		crc_check_list = <0x0200 0x15FB 0x15FC
						0x1D00 0x2D2B 0x2D2C
						0x2D30 0x2D5B 0x2D5C
						0x2D60 0x2D9B 0x2D9C
						0x2DA0 0x2E0B 0x2E0C
						0x2E10 0x2E6B 0x2E6C
						0x2E70 0x2E9B 0x2E9C
						0x1D00 0x2EDF 0x2EFC
						0x2F00 0x2FFB 0x2FFC
						0x3000 0x4499 0x449A
						0x3000 0x44DF 0x44FC>;

		/* Master */
		rom_header_version_start_addr = <0x50>;
		rom_header_cal_data_start_addr = <0x200>;
		rom_header_cal_map_ver_start_addr = <0x70>;
		rom_header_isp_setfile_ver_start_addr = <0x74>;
		rom_header_project_name_start_addr = <0x7E>;
		rom_header_module_id_addr = <0xAE>; /* 0xA8 + reserved bytes */
		rom_header_sensor_id_addr = <0xB8>;
		rom_header_mtf_data_addr = <0xE0>;
		rom_paf_cal_start_addr = <0x1D00>;
		rom_awb_master_addr = <0x2D60>;
		rom_awb_module_addr = <0x2D84>;
		/* Near Far Mid1 ... Mid7 */
		rom_af_cal_addr = <0x2D40 0x2D34 0x2D38 0x2D3C>;

		/* rom_xtc_cal_endian_check; */
		rom_xtc_cal_data_addr_list = <0x3000 0x4499 0x1>;

 		/* sensor ois - xgg ygg xcoef ycoef xsupper ysupper calmark */
		/* sensor2 ois - xgg ygg xcoef ycoef xsupper ysupper calmark */
		/* TO DO */
		rom_ois_list = <0x2E20 0x2E24 0x2E2C 0x2E2E 0x2E48 0x2E4A 0x2E40
				0x3630 0x3634 0x363C 0x363E 0x3658 0x365A 0x3650>;
	};
};

