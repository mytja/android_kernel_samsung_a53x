	tele_rst_sleep: tele-rst-sleep {
		samsung,pins = "gpp7-3";
		samsung,pin-con-pdn = <0>;
	};
};

&pinctrl_7 {
	/* Tele AF */
	hsi2c6_bus_in: hsi2c6-bus-in {
		samsung,pins = "gpp3-0", "gpp3-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
		samsung,pin-pud-pdn = <0>;
	};

	/* Front */
	hsi2c7_bus_in: hsi2c7-bus-in {
		samsung,pins = "gpp3-2", "gpp3-3";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
		samsung,pin-pud-pdn = <0>;
	};

	/* Front RST */
	vt_rst_sleep: vt-rst-sleep {
		samsung,pins = "gpg0-1";
		samsung,pin-con-pdn = <0>;
	};

	/* torch */
	torch_sleep: torch-sleep {
		samsung,pins = "gpc4-0";
		samsung,pin-con-pdn= <3>;
	};

	/* ois spi 0*/
	ois_spi0: ois-spi0 {
		samsung,pins = "gpp6-0", "gpp6-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <1>;
	};

	/* ois spi 1*/
	ois_spi1: ois-spi1 {
		samsung,pins = "gpp6-2", "gpp6-3";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-con-pdn = <2>;
		samsung,pin-pud-pdn = <0>;
	};
};

&pinctrl_1 {
	/* Ultra Wide */
	hsi2c30_bus_in: hsi2c30-bus-in {
		samsung,pins = "gpm6-0", "gpm6-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* Front AF */
	hsi2c31_bus_in: hsi2c31-bus-in {
		samsung,pins = "gpm7-0", "gpm7-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
		samsung,pin-pud-pdn = <0>;
	};

	hsi2c31_bus: hsi2c31-bus {
		samsung,pin-drv = <3>;
	};
};

&hsi2c_6 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpp3 0 0 &gpp3 1 0>;
	status = "okay";
	clock-frequency = <1000000>;
	samsung,reset-before-trans;
	samsung,fast-plus-mode;

	pinctrl-names = "default","on_i2c","off_i2c";
	pinctrl-0 = <&hsi2c6_bus_in>;
	pinctrl-1 = <&hsi2c6_bus>;
	pinctrl-2 = <&hsi2c6_bus_in>;

	is-actuator@0F { /* tele AF */
		compatible = "samsung,exynos-is-actuator-ak737x";
		sensor-module = <&is_sensor_3k1>;
		reg = <0x0F>;
		id = <2>; /* matching is_sensor id */
		/* AK737x: addr 0xF3, data 0x0E */
		vendor_product_id = <0xF3 0x0E 0xF3 0x13 0xF3 0x00>;
		vendor_first_pos = <2000>;  /* 12bits */
		vendor_first_delay = <2000>;
		vendor_soft_landing_seqid = <2>;
		vendor_soft_landing_list = <640 30 64 30>; /* 1st_pos delay 2nd_pos delay */  /* 12bits */
		vendor_use_sleep_mode;
		vendor_use_standby_mode; /* AK7377 with vendor_use_sleep_mode */
	};

	rear3_eeprom@52 { /* tele x3 */
		compatible = "samsung,rear3-eeprom-i2c";
		reg = <0x52>;

		rom_power_position = <2>;
		rom_size = <0x4000>;
		cal_map_es_version = <23>;
		camera_module_es_version = "A";
		/* skip_cal_loading; */

		/* Rainbow_project_R0G0_Rear_Cal_Map_V023_20210520_for_all */
		/* 0 header */
		header_crc_check_list = <0x0000 0x01FB 0x01FC>;
		/* 0 LSI Wide LSC data */
		/* 1 Tele camera */
		/* 2 ATC data */
		crc_check_list = <0x0200 0x15FB 0x15FC
						0x1D00 0x2D2B 0x2D2C
						0x2D30 0x2D5B 0x2D5C
						0x2D60 0x2D9B 0x2D9C
						0x2DA0 0x2E0B 0x2E0C
						0x2E10 0x361B 0x361C
						0x3620 0x367B 0x367C
						0x1D00 0x36DF 0x36FC
						0x3700 0x37FB 0x37FC>;

		/* Master */
		rom_header_version_start_addr = <0x40>;
		rom_header_cal_data_start_addr = <0x200>;
		rom_header_cal_map_ver_start_addr = <0x70>;
		rom_header_isp_setfile_ver_start_addr = <0x74>;
		rom_header_project_name_start_addr = <0x7E>;
		rom_header_module_id_addr = <0xAE>; /* 0xA8 + reserved bytes */
		rom_header_sensor_id_addr = <0xC8>;
		rom_header_mtf_data_addr = <0x182>;
		rom_awb_master_addr = <0x2D60>;
		rom_awb_module_addr = <0x2D84>;
		/* Near Far Mid1 ... Mid7 */
		rom_af_cal_addr = <0x2D40 0x2D34 0x2D38 0x2D3C>;

		/* Dual cal slave 0 - Tele */
		rom_dualcal_slave0_start_addr = <0x2E10>;
		rom_dualcal_slave0_size = <0x080C>; /* 2060 */
		/* x y z sx sy */
		/* range max_err avg_err dll_version prj_name */
		rom_dualcal_slave1_tilt_list = <0x2EC8 0x2ECC 0x2ED0 0x2EEC 0x2EF0
						0x30E2 0x30E6 0x30EA 0x3104 0x30EF>;

		/* sensor ois - xgg ygg xcoef ycoef xsupper ysupper calmark */
		/* sensor2 ois - xgg ygg xcoef ycoef xsupper ysupper calmark */
		/* TO DO */
		rom_ois_list = <0x2E20 0x2E24 0x2E2C 0x2E2E 0x2E48 0x2E4A 0x2E40
				0x3630 0x3634 0x363C 0x363E 0x3658 0x365A 0x3650>;
	};
};

